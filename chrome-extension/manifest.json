{
    "manifest_version": 3,
    "name": "AI-UI Explorer",
    "description": "Tools to explore and create AI-UI tags",
    "version": "1.0.0",
    "author": "MatAtBread",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "permissions": ["activeTab","scripting"],
    "action": {
      "default_popup": "newtab.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    }
  }
  